{
  "compilerOptions": {
    "target": "ES2021",
    "outDir": "dist",
    "rootDir": "src",
    "composite": false,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "lib": ["ES2021", "DOM", "DOM.Iterable"],
    "types": ["node"],
    "paths": {}
  },
  "include": ["src"]
}
