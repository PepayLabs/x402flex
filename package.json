{
  "name": "@pepaylabs/x402flex",
  "version": "0.2.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./evm": {
      "types": "./dist/evm/index.d.ts",
      "default": "./dist/evm/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    },
    "./fetch": {
      "types": "./dist/fetch/index.d.ts",
      "default": "./dist/fetch/index.js"
    },
    "./axios": {
      "types": "./dist/axios/index.d.ts",
      "default": "./dist/axios/index.js"
    },
    "./express": {
      "types": "./dist/express/index.d.ts",
      "default": "./dist/express/index.js"
    },
    "./package.json": "./package.json"
  },
  "description": "Canonical Pepay Labs x402Flex SDK for API-mode and direct-contract integrations.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "test": "vitest run"
  },
  "dependencies": {
    "@bnbpay/sdk": "file:../sdk-ts",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.2",
    "vitest": "^4.0.8"
  }
}
